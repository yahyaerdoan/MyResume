@model  List<MyResume.Entities.Education>
<section class="resume-section p-3 p-lg-5 d-flex flex-column" id="education">
    <div class="my-auto">
        <h2 class="mb-5">Education</h2>

        <div class="resume-item d-flex flex-column flex-md-row mb-5">
            @foreach (var item in Model)
            {
                <div class="resume-content mr-auto">
                    <h3 class="mb-0">@item.SchoolName</h3>
                    <div class="subheading mb-3">@item.FieldOfStudy</div>
                    <div>@item.Grade - @item.Degree</div>
                    <p>GPA: @item.GradePointAverage</p>
                    <p>@item.Description</p>
                </div>
                <div class="resume-date text-md-right">
                    <span class="text-primary">@item.StartingMonth.ToString("MMMM") @item.StartingYear.Year - @item.EndingMonth.ToString("MMMM") @item.EndingYear.Year</span>
                </div>
            }
        </div>
    </div>
</section>
