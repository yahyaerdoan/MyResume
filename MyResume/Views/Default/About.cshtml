@model List<MyResume.Entities.About>
<section class="resume-section p-3 p-lg-5 d-flex d-column" id="about">
    <div class="my-auto">
        @foreach (var item in Model)
        {
            <h1 class="mb-0">
                @item.FirstName
                <span class="text-primary">@item.LastName</span>
            </h1>

            <div class="subheading mb-5">
                @item.Address, @item.City, @item.State @item.ZipCode, @item.Country · @item.PhoneNumber ·
                <a href="mailto:@item.Email">yahyaerdoan@email.com</a>
            </div>
            <p class="mb-5">@item.Description</p>
        }
        @Html.Action("SocialMedia", "Default")
    </div>
</section>
