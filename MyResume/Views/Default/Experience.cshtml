@model List<MyResume.Entities.Experience>
<section class="resume-section p-3 p-lg-5 d-flex flex-column" id="experience">
    <div class="my-auto">
        <h2 class="mb-5">Experience</h2>
        @foreach (var item in Model)
        {
            <div class="resume-item d-flex flex-column flex-md-row mb-5">
                <div class="resume-content mr-auto">
                    <h3 class="mb-0">@item.Header</h3>
                    <div class="subheading mb-3">@item.Title</div>
                    <p>@item.Description</p>
                </div>
                <div class="resume-date text-md-right">
                    <span class="text-primary">@item.StartingMonth.ToString("MMMM")-@item.StartingYear.Year / @item.EndingMonth.ToString("MMMM")-@item.EndingYear.Year</span>
                </div>
            </div>
        }
    </div>

</section>
